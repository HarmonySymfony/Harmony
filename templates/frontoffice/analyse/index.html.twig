{% extends 'base.html.twig' %}

{% block body %}

    <div style="padding-top: 200px" class="container">
        <h1>Analyses index</h1>
    <table class="table">
        <thead>
            <tr>
         
                <th>Type</th>
                <th>Prix</th>

            </tr>
        </thead>
        <tbody>
        {% for analyse in analyses %}
            <tr>
            
                <td>{{ analyse.type }}</td>
                <td>{{ analyse.prix }}</td>
               
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}
